<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="in.artist.database.classes.AppConfig" table="appconfig">

		<id column="id" name="appConfigId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property column="appKey" name="appKey" type="java.lang.String" />
		<property column="appValue" name="appValue" type="java.lang.String" />
		
	</class>
</hibernate-mapping>
