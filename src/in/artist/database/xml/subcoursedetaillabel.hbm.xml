<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="in.artist.database.classes.SubCourseDetailLabel" table="subcoursedetaillabel">

		<id column="id" name="subCourseDetailLabelId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property column="subCourseDetailId" name="subCourseDetailId" type="java.lang.Integer" />
		<property column="iconUrl" name="iconUrl" type="java.lang.String" />
		<property column="description" name="description" type="java.lang.String" />
	</class>
</hibernate-mapping>