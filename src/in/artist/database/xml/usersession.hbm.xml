<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="in.artist.database.classes.UserSession" table="usersession">

		<id column="id" name="sessionId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		
		<property column="accessToken" name="accessToken" type="java.lang.String" />
		<property column="uid" name="uid" type="java.lang.Integer" />
		<property column="created" name="created" type="java.lang.Long" />
		<property column="modified" name="modified" type="java.lang.Long" />
		
	</class>
</hibernate-mapping>